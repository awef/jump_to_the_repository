{
  "name": "Jump to the repository",
  "version": "0.0",
  "manifest_version": 2,
  "description": "GitHub Pagesからリポジトリのページにジャンプするボタンを表示します",
  "icons": {
    "16": "icon_16x16.png",
    "48": "icon_48x48.png",
    "128": "icon_128x128.png"
  },
  "page_action": {
    "default_icon": "icon_19x19.png",
    "default_title": "Jump to the repository"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "incognito": "split",
  "minimum_chrome_version": "18",
  "permissions": [
    "tabs"
  ]
}
